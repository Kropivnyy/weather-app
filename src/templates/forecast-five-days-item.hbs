{{#each this}}
<li class="five-days__one-day-item">
    <p class="five-days__day-of-week">{{date.day}}</p>
    <p class="five-days__date-and-month">
        <span class="five-days__date">{{date.date}}</span><span class="five-days__month">{{date.month}}</span>
    </p>
    <img class="five-days__img-of-weather" src="{{icon}}" height="22px" width="44px" alt="" />
    <ul class="five-days__temperature-info-list">
        <li class="five-days__temperature-info-item">
            <p class="five-days__temperature-info-sign">min</p>
            <p class="five-days__temperature-min">
                {{temp.temp_min}}°
            </p>
        </li>
        <li class="five-days__temperature-info-item">
            <p class="five-days__temperature-info-sign">max</p>
            <p class="five-days__temperature-max">
                {{temp.temp_max}}°
            </p>
        </li>
    </ul>
    <a class="five-days__more-info" href="">more info</a>
</li>
{{/each}}